
@prefix rmls: <http://semweb.mmlab.be/ns/rmls#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .

@base <http://example.com/base/> .

<TriplesMap1>
  a rr:TriplesMap;
  rml:logicalSource [
    rml:source _:kafka_source;
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.students[*]"
  ];
    
    rr:subjectMap [ rml:reference "Name"; rr:termType rr:IRI; ];
    
    rr:predicateObjectMap 
    [
    	rr:predicate rdf:type;
    	rr:object foaf:Person;
    ].

_:kafka_source  rdf:type rmls:KafkaStream ;
             rmls:zookeeper "localhost:2181";
             rmls:broker "localhost:9092";
             rmls:groupId "2";
             rmls:topic "demo".

