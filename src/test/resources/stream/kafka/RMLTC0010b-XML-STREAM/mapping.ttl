@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rmls: <http://semweb.mmlab.be/ns/rmls#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix activity: <http://example.com/activity/> .
@base <http://example.com/base/> .


<TriplesMap1>
  a rr:TriplesMap;

  rml:logicalSource [ 
    rml:source [ rdf:type rmls:KafkaStream ;  rmls:zookeeper "localhost:2181"; rmls:broker "localhost:9092"; rmls:groupId "2";  rmls:topic "demo"         ];

        rml:referenceFormulation ql:XPath;
    rml:iterator "/countries/country"
  ];

  rr:subjectMap [ rr:template "http://example.com/{CountryCode}/{Name}" ];
	
  rr:predicateObjectMap [ 
    rr:predicate ex:name ;
    rr:objectMap [ rml:reference "Name" ]
  ] .
